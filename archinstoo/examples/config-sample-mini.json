{
    "bug_report_url": "https://github.com/h8d13/archinstoo",
    "script": "guided",
    "archinstall-language": "English",
    "locale_config": {
        "kb_layout": "us",
        "sys_lang": "en_US.UTF-8",
        "sys_enc": "UTF-8"
    },
    "pacman_config": {
        "mirror_regions": {
            "Germany": [
                "https://frankfurt.mirror.pkgbuild.com/$repo/os/$arch",
                "https://berlin.mirror.pkgbuild.com/$repo/os/$arch"
            ]
        },
        "custom_servers": [],
        "optional_repositories": [
            "multilib"
        ],
        "custom_repositories": [],
        "pacman_options": [
            "Color",
            "ILoveCandy"
        ]
    },
    "bootloader_config": {
        "bootloader": "Grub",
        "uki": true,
        "removable": true
    },
    "swap": {
        "enabled": true,
        "algorithm": "lz4"
    },
    "kernels": [
        "linux-zen"
    ],
    "kernel_headers": true,
    "profile_config": {
        "profile": {
            "main": "Desktop",
            "details": [
                "KDE Plasma"
            ],
            "custom_settings": {
                "KDE Plasma": {}
            }
        },
        "gfx_driver": "Intel (open-source)",
        "greeter": "gdm"
    },
    "hostname": "archinstoo",
    "auth_config": {
        "lock_root_account": true,
        "privilege_escalation": "doas"
    },
    "app_config": {
        "bluetooth_config": {
            "enabled": true
        },
        "audio_config": {
            "audio": "pipewire"
        },
        "print_service_config": {
            "enabled": true
        },
        "firewall_config": {
            "firewall": "ufw"
        },
        "management_config": {
            "tools": [
                "git",
                "base-devel",
                "man-db",
                "pacman-contrib",
                "reflector"
            ]
        },
        "monitor_config": {
            "monitor": "bottom"
        },
        "editor_config": {
            "editor": "micro"
        }
    },
    "network_config": {
        "type": "nm_iwd"
    },
    "parallel_downloads": 0,
    "timezone": "Europe/Berlin",
    "ntp": true,
    "packages": [],
    "services": [],
    "custom_commands": []
}
